{
  "name": "@bushy-packs/bushy-packs",
  "version": "0.1.3",
  "description": "Bushy Packs is a CLI software that allows you to compile a list of repos and variables into game servers.",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/bp",
    "dev": "npm run build && npm run start",
    "lint": "eslint ./src --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "test": "npm run build && jest",
    "test-run": "npm run start -- -c busheezy/bushy-packs-test-config -d ./tests/test-output -p test -l ./tests/test-output/lgsm",
    "test-run2": "npm run start -- -c tarikgg/tarik-server-configs -d ./tests/test-output -p nyc.pistol.dm --key id_rsa_tarikgg"
  },
  "main": "dist/bp.js",
  "dependencies": {
    "bluebird": "^3.5.3",
    "commander": "^2.20.0",
    "fs-extra": "^7.0.1",
    "handlebars": "^4.1.2",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "nodegit": "^0.24.0",
    "pjson": "^1.0.9",
    "simple-vdf": "^1.1.1",
    "through2": "^3.0.0"
  },
  "bin": {
    "bp": "./dist/bp.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/bluebird": "^3.5.25",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^5.0.4",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.120",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.12.18",
    "@types/nodegit": "^0.22.7",
    "@types/through2": "^2.0.34",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.8.0",
    "prettier": "1.16.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.2.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/busheezy/bushy-packs.git"
  },
  "author": "Ryan Bucshon"
}
